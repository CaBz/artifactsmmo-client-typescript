<%- include('../partials/header'); %>

<h2>Bank: (<%= bank.items.length %>/<%= bank.bank.slots %>) with <%=bank.bank.gold%>g</h2>

<table class="table table-striped">
    <thead>
    <tr>
        <th scope="col">#</th>
        <th scope="col">Code</th>
        <th scope="col">Name</th>
        <th scope="col">Type</th>
        <th scope="col">Subtype</th>
        <th scope="col">Quantity</th>
    </tr>
    </thead>
    <tbody>
    <% bank.items.forEach(function(bankItem, index){ %>
    <tr>
        <th scope="row"><%=index+1%></th>
        <td><%=bankItem.value.code %></td>
        <td><%=bankItem.value.name %></td>
        <td><%=bankItem.value.type %></td>
        <td><%=bankItem.value.subType %></td>
        <td><%=bankItem.quantity %></td>
    </tr>
    <% }); %>
    </tbody>
</table>

<%- include('../partials/footer'); %>
